        "\n",
        "var = random.randint(0, 100)\n",
        "\n",
        "if var % 3 == 0 and var % 5 == 0:\n",
        "  print(\"Fizz\")\n",
        "\n",
        "elif var % 3 == 0:\n",
        "  print(\"Buzz\")\n",
        "\n",
        "elif var % 5 == 0:\n",
        "  print(\"FizzBuzz\")\n",
        "\n",
        "else:\n",
        "  print(var)"
      ]
